{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {
          
   },
    "examples": [
        {
            "payer_id": 1,
            "payerName": "abc",
            "payment_type": "IPH",
            "payerAccountNumber": 12345
        }
    ],
    "required": [
        "payer_id",
        "payerName",
        "payment_type",
        "payerAccountNumber"
    ],
    "properties": {
        "payer_id": {
            "$id": "#/properties/payer_id",
            "type": ["integer","null"],
             "default":123456,
           	 "title": "The payer_id schema",
            "description": "An explanation about the purpose of this instance.",
           
            "examples": [
                1
            ]
        },
        "payerName": {
            "$id": "#/properties/payerName",
            "type": ["string","null"],
            "title": "The payerName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "abc"
            ]
        },
        "payment_type": {
            "$id": "#/properties/payment_type",
            "type": ["string","null"],
            "title": "The payment_type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "IPH"
            ]
        },
        "payerAccountNumber": {
            "$id": "#/properties/payerAccountNumber",
            "type": ["integer","null"],
            "title": "The payerAccountNumber schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                12345
            ]
        }
    },
    "additionalProperties": true
}